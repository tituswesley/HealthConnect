<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Care Plan Management">
    
  <listeners>
	<listener class-name="listener.MyTestNGListener"></listener>
  </listeners>
  <test name="Create CarePlan Test">
    <classes>
      <class name="testcases.hc.CarePlan">
       <methods>
           <include name="createCarePlan" />
       </methods>    
      </class>
    </classes>
  </test>
   <test name="Edit CarePlan Test">
    <classes>
      <class name="testcases.hc.CarePlan">
       <methods>
           <include name="editCarePlan" />
       </methods>    
      </class>
    </classes>
  </test>
</suite> <!-- Suite -->
